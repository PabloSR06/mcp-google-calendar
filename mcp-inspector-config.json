{
  "mcpServers": {
    "mcp-google-calendar": {
      "command": "npx",
      "args": ["-y", "@pablosr/mcp-google-calendar"],
      "env": {
        "GOOGLE_CLIENT_ID": "${GOOGLE_CLIENT_ID}",
        "GOOGLE_CLIENT_SECRET": "${GOOGLE_CLIENT_SECRET}",
        "GOOGLE_REDIRECT_URL": "${GOOGLE_REDIRECT_URL}",
        "GOOGLE_REFRESH_TOKEN": "${GOOGLE_REFRESH_TOKEN}"
      }
    }
  }
}
